<UserControl x:Class="Zoom.PE.DesignTime.SilverlightControl1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:Zoom.PE"
    xmlns:DesignTime="clr-namespace:Zoom.PE.DesignTime"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">
    <UserControl.Resources>
        <DesignTime:ModelSource x:Key="ModelSource"/>
    </UserControl.Resources>
    
    <Grid x:Name="LayoutRoot" Background="White">
        
        <ContentControl Content="{Binding PEFileModel, Source={StaticResource ModelSource}}"/>
        
        

    </Grid>
</UserControl>
